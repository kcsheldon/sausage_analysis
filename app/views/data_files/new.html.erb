<%= form_for @data_file do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <div class="field button">
    <%= f.label :profile_photo %>
    <%= f.file_field :profile_photo %>
  </div>

  <%= f.submit "Add Data File", class: :button %>
  <% end %>
